<p id="notice"><%= notice %></p>

<h1>Duenos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @duenos.each do |dueno| %>
      <tr>
        <td><%= link_to 'Show', dueno %></td>
        <td><%= link_to 'Edit', edit_dueno_path(dueno) %></td>
        <td><%= link_to 'Destroy', dueno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dueno', new_dueno_path %>
